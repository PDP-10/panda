cusys:alarm -
cusys:chdir -
cusys:chmod -
cusys:chown -
cusys:close -
cusys:dup -
cusys:exit -
cusys:fcntl -
cusys:forkex -
cusys:getpid -
cusys:getuid -
cusys:ioctl -
cusys:lseek

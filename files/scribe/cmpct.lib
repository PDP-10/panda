@Marker(Library,CMPCT)
@String(CACM="Comm. ACM",JACM="J. ACM",
	IEEETC="IEEE Trans. Computers",
	IBMJR="IBM J. Res. and Develop.",
	IBMJST="IBM J. Syst. and Tech.",
	SIAMJC="SIAM J. Comput.")
@String(CMU="Carnegie-Mellon University",
	CMUCSD="Carnegie-Mellon University Computer Science Department",
	USCISI="USC/Information Sciences Institute")
@String(jan="January",feb="February",mar="March",apr="April",may="May",
	jun="June",jul="July",aug="August",sep="September",
	oct="October",nov="November",dec="December")
@BibForm(Book=<

@l1{@Tagger(@parm(Tag))@parm(Author).
@imbed(Series,def '@i{@parm(Series).}  Volume @parm(Volume): @i{@parm(Title).}',
	      undef '@i{@parm(Title).}')
@imbed(HowPublished,def '@Parm(HowPublished).  ')@Parm(Publisher)@imbed(Address,def ', @parm(Address)'), @parm(year).@imbed(note,def '  @parm(Note)')}
>)

@BibForm(Article=<

@l1{@Tagger(@parm(Tag))@parm(Author).
@parm(Title).
@i{@parm(Journal) @parm(Volume)}@imbed(number,def ', @parm(number)') (@imbed(month,def '@parm(month) ')@parm(year)), @parm(Pages).@imbed(note,def '  @parm(Note)')}
>)

@BibForm(InBook=<

@l1{@Tagger(@parm(Tag))@Parm(Author).
@Parm(Title).
In @imbed(Editor,def '@Parm(Editor), Ed., ')@i[@Parm(BookTitle)],
@Parm(Publisher)@imbed(Address,def ', @parm(Address)'), @[[
]]@parm(year)@imbed(chapter,def ', Chap. @parm(Chapter)')@[[
]]@imbed(pages,def ', pp. @parm(pages)').@imbed(note,def '  @parm(Note)')}
>)

@BibForm(InProceedings=<

@l1{@Tagger(@parm(Tag))@Parm(Author).
@Parm(Title).
@parm(Booktitle),
@imbed(Organization,def '@Parm(Organization), ')@[[
]]@imbed(Address,def '@parm(Address), ')@imbed(Month,def '@parm(Month),@[[
]] ')@parm(Year)@imbed(pages,def ', pp. @parm(Pages)').@imbed(note,def '  @parm(Note)')}
>)

@BibForm(Proceedings=<

@l1{@Tagger(@parm(Tag))@imbed(Organization,def ', @Parm(Organization).')
@i[@Parm(Title)]@imbed(Address,def ', @parm(Address)')@imbed(Month,def ', @parm(Month)'), @parm(Year).@imbed(note,def '  @parm(Note)')}
>)

@BibForm(PhDThesis=<

@l1{@Tagger(@parm(Tag))@parm(Author).
@i{@parm(Title)}.
Ph.D. Th., @parm(School)@imbed(address, def ', @parm(address)'),@imbed(Month,def ' @parm(month)') @parm(year).@imbed(note,def '  @parm(Note)')}
>)

@BibForm(MastersThesis=<

@l1{@Tagger(@parm(Tag))@parm(Author).
@parm(Title).
Master Th., @parm(School)@imbed(address,def ', @parm(address)'),@imbed(Month,def ' @parm(month)') @parm(year).@imbed(note,def '  @parm(Note)')}
>)

@BibForm(TechReport=<

@l1{@Tagger(@parm(Tag))@parm(Author).  @parm(Title).
@imbed(Number,def '@imbed(type, def {@parm(type) })@parm(Number), ')@parm(Institution), @imbed(Month,def '@parm(Month), ')@parm(year).@imbed(note,def '  @parm(Note)')}
>)

@BibForm(Booklet=<

@l1{@Tagger(@parm(Tag))@parm(Author).
@parm(Title).
@imbed(HowPublished,def '@Parm(HowPublished).  ')@imbed(Address,def '@parm(Address), '),@imbed(Month,def '@parm(Month), ')@parm(year).@imbed(note,def '  @parm(Note)')}
>)

@BibForm(UnPublished=<

@l1{@Tagger(@parm(Tag))@Parm(Author).
@Parm(Title).  @parm(Note)}
>)

@BibForm(Misc=<

@l1{@Tagger(@parm(Tag))@parm(Author).
@parm(Title).
@parm(HowPublished).@imbed(note,def '  @parm(Note)')}
>)


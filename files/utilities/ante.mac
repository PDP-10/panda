IFN F.SAIL,<
PSI$CC:	SKIPN	FLAGCD
	 JRST	PSI$C0
	SETOM	FLAGCC
	DISMIS
PSI$C0:	UWAIT
	DEBREA
	OUTSTR	[ASCIZ/^C/]
	EXIT	1,
	MOVE	PTR,PSI$PC
	CAIN	PTR,<INNEXT+1>

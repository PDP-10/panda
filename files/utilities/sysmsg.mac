TITLE SYSMSG
SEARCH MONSYM

	SKIPA 6,[4]		; sysall
GO:	 MOVEI 6,3		; sysmsg
	MOVSI 1,(GJ%SHT!GJ%OLD)	; find BBOARD program
	HRROI 2,[ASCIZ "SYS:BBOARD.EXE"]
	GTJFN%
	 NOP
	HRLI 1,.FHSLF		; load bootstrap into ACs
	DMOVE 2,[GET%		; get BBOARD into core
		 MOVEI 1,.FHSLF]
	DMOVE 4,[MOVE 2,6
		 SFRKV%]	; start at appropriate offset into entry vector
	JRST 2

END GO

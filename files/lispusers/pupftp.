(PUTPROPS .COERCEUSER. MACRO [(HOST USER PWD)
			      (COND
				([AND (NOT USER)
				      (SETQ USER (OR (CAR (GETHASH HOST LOGINPASSWORDS))
						     (CAR (GETHASH NIL LOGINPASSWORDS))
						     (\INTERNAL/GETPASSWORD HOST]
				  (SETQ PWD (\DECRYPT.PWD (CDR USER)))
				  (SETQ USER (CAR USER])
)

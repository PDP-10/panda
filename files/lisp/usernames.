(DEFINEQ

(MAKEUSERNAMES
  [LAMBDA NIL                                   (* lmm: 24 MAY 75 1739)
    (PRINT (QUOTE (Be sure to add the name of new users to <LISP>USERS))
	   T)
    (PRIN1 (PRETTYDEF (QUOTE (MAKEUSERNAMES))
		      (QUOTE <LISP>USERNAMES)
		      (QUOTE (USERNAMELST)))
	   T)
    (PRIN1 (QUOTE ,)
	   T)
    (PROG (N POS FL)
          (OUTFILE USERNAMEFILE)
          (SETQ FL (OUTPUT))
          (RPTQ 52 (PROGN (RPTQ 9 (PROGN (PRIN1 0)
					 (SPACES 5)))
			  (PRIN1 0)
			  (SPACES 3)
			  (TERPRI)))
          [MAPC USERNAMELST (FUNCTION (LAMBDA (X)
		    (COND
		      ([NULL (SETQ N (COND
				 ((CAR X)
				   (USERNUMBER (CAR X)))
				 (T 

          (* An entry of the form (NIL --) means forms on CDR are 
	  evaluated for everyone.)


				    0]
			(PRIN1 (CAR X)
			       T)
			(PRIN1 (QUOTE " NOT A USER.")
			       T))
		      (T (SETQ POS (GETFILEPTR FL))
			 (SETFILEPTR FL (ITIMES N 6))
			 (PRIN1 POS)
			 (SETFILEPTR FL POS)
			 (PRINT X]
          (PRINT (CLOSEF FL))
          (DIR (QUOTE USERNAME)
	       (QUOTE (P PROMPT "trim? " TRIMTO 1])
)
(PRETTYCOMPRINT (MAKEUSERNAMES))
(PRETTYCOMPRINT (USERNAMELST))

(RPAQQ USERNAMELST ([NIL
	  [/PUTD (QUOTE DISPLAYTERMP)
		 (QUOTE (LAMBDA NIL (EQ (JSYS 195 262143 0 0 2)
					11]
	  (SETQQ LISTFILESTR "
Y
")
	  (SETQQ HELPSYSDIRLST (<LISP>))
	  (SETQ CHANGESARRAY)
	  [OR (GETD (QUOTE PRETTYCOMPRINT))
	      (PUTD (QUOTE PRETTYCOMPRINT)
		    (QUOTE (NLAMBDA (X)
				    (LISPXPRINT X T T]
	  (COND
	    ((EQ T (EVALV (QUOTE SPAGHETTIFLG)))
	     (PUTPROPS SETBACKSPACE FILEDEF (SETBACKSPACE.SCOM SETBACKSPACE)))
	    (T
	      (PUTPROPS SETBACKSPACE FILEDEF (SETBACKSPACE.COM SETBACKSPACE))
	      (COND
		((STRPOS "-75 " MAKESYSDATE)
		 (CHANGENAME (QUOTE BCOMPL)
			     (QUOTE BCOMPL.SCRATCH;S)
			     (QUOTE BCOMPL.SCRATCH;-1;T))
		 (CHANGENAME (QUOTE BRECOMPILE)
			     (QUOTE BCOMPL.SCRATCH;S)
			     (QUOTE BCOMPL.SCRATCH;-1;T)))
		(T
		  (PUTPROPS PACK ARGNAMES (U))
		  [AND
		    (EVALV (QUOTE PACKADVISED))
		    (PROG
		      (LISPXHIST)
		      [RESETVAR
			ADVISEDFNS ADVISEDFNS
			(ADVISE
			  (QUOTE (PACK IN COMPBLOCK))
			  (QUOTE BEFORE)
			  (QUOTE (MAP U (FUNCTION
					(LAMBDA
					  (L)
					  (OR (ATOM (CAR L))
					      (RPLACA L (EVALV (QUOTE FN)
							       (QUOTE COMP]
		      (SET (QUOTE PACKADVISED]
		  (SETQ MAKEFILEREMAKEFLG T)
		  (SETQ EDITLOADFNSFLG T]

(DEFINEQ

(NTHCHARCODE
  [LAMBDA (X N FLG RDTBL)          (* lmm "10-AUG-81 21:12")
                                   (* DCODEFOR10 VERSION; TURNS EOL INTO CR)
    (PROG NIL
          [COND
	    [FLG (RETURN (CHCON1 (NTHCHAR X N FLG RDTBL]
	    ((STRINGP X)
	      NIL)
	    [(LITATOM X)
	      (SETQ X (CDR (VAG (IPLUS (LOC X)
				       2]
	    (T (RETURN (CHCON1 (NTHCHAR X N FLG RDTBL]
          (RETURN (ASSEMBLE NIL
			    (CQ (VAG (FIX N)))
			    (MOVE 7 , 1)
			    (CQ X)
			    (FASTCALL UPATM)
			    (SKIPGE 7)
			    (ADDI 7 , 1 (4))
			    (JUMPLE 7 , FALSE)
			    (CAILE 7 , 0 (4))
			    (JRST FALSE)
			    (SUBI 7 , 1)
			    (IDIVI 7 , 5)
			    (ADDI 3 , 0 (7))
			    (IBP 3)
			    (SOJGE 10Q , * -1)
			    (LDB 1 , 3)
			    (CAIN 1 , 37Q)
                                   (* turn EOL into CR)
			    (MOVEI 1 , 15Q)
			    (ADDI 1 , ASZ)
			    (JRST OUT)
			FALSE
			    (CQ NIL)
			OUT])


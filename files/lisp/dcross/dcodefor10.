(DEFINEQ

(NEWARRAYSIZE
  [LAMBDA (A)                      (* lmm "11-AUG-81 23:48")
    (SELECTQ (GETHASH A ARRAYTYPHA)
	     (CODE (UNFOLD (IDIFFERENCE (OLDARRAYSIZE A)
					3)
			   BYTESPERCELL))
	     (WORD (LLSH (OLDARRAYSIZE A)
			 1))
	     (OLDARRAYSIZE A])
)

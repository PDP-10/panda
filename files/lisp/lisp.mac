	CAIL 2,3
	SETZM 2(1)
	CAIG 2,3
	RET			;NOTHING TO CLEAR
	HRLZI 3,2(1)
	HRRI 3,3(1)
	ADDI 2,-1(1)
	BLT 3,0(2)
HSHLMT:	HRRZ 2,0(1)	; GET NEW ARRAYSIZE
	SUBI 2,2
	MOVEI 3,0(2)
	LSH 3,-3	;GET 1/8 OF SIZE
	SUBI 3,0(2)	;COMPUTE -7/8 OF SIZE

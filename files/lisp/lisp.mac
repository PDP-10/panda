	CAILE 1,MSYST
	CAIL 1,MTYPN
	JRST FALSE
	HRLZI 4,-1
	HRR 4,OBJTNS(1)
	HRRZ 2,2(VP)
	CAMN 2,KNIL
	SETZ 2,
	HRRI 4,0(2)
	EXCH 4,OBJTNS(1)
	HRRZ 1,4
	SKIPN 1
	HRRZ 1,KNIL

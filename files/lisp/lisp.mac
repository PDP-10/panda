CPCOP1:	HRRZ 4,ICP
CPCOP3:	CAIN 3,1(4)		;SKIP THE ONE WE ARE TRYING TO COPY
	ADD 4,PPT		;... CPO MAY BE WRONG
	HLRZ 5,1(4)
	CAIN 5,STKHOL		;HOLE FOLLOWS?
	JRST CPCOP2
	CAIN 5,STKEND		;OR END OF BLOCK?
	JRST CPCOP7
	HLRZ 4,CPOWD+1(4)		;NO - CPO IS NEXT END

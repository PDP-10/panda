	JRST STKAN3

STKAN2:	PUSH PP,3		;SAVE NEW NAME
	PUSH PP,2		;SAVE FRAME
	CALL FNDBND		;FIND THE OLD BINDING
	POP PP,2		;GET BACK FRAME

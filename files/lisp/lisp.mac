	SKIPA 3,2
EXCALQ:	HRRZ 3,40
	CAMN 3,KNIL
	JRST EXILL
	CARA 5,3
	CAME 5,KLAM
	CAMN 5,KNLA
	JRST EXCLM
EXILL:	PUSH PP,2		;FN NAME
	HRRZ 3,CF		;FINISH FRAME
	SETNAR 1,3
	SETPPI PP,3
	PUSH PP,2
	MOVEI 6,-2
	HRLI 6,PP
	CALL LSTAR2
	PUSH PP,1
	LCALL KFALTA,2
	RET
EXCLM:	CDRA 3,3

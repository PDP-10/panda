	TLZ 1,-1		;MAKE SURE LEFT HALF IS ZERO FOR FNDBND
	RET

STKNRG:	CALL STKGP
	JUMPE 1,STKER1
	GETNAR 1,1		;CHANGE FOR COMPILER?????????
	JRST MKN




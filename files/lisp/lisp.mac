CPCOP7:	HRRZ 4,1(4)		;NEXT STACK REGION
	JUMPE 4,@CCOPX		;NO MORE STACK AT ALL
	CAME 4,IREDCP		;LAST STACK REGION?
	JRST CPCOP3		;NO - JUST GO ON
	MOVE 5,STKMOD		;YES
	TROE 5,SMCR		;ALREADY IN EMER. MODE?

	PUSH PP,2
	HRRZ 1,3(VP)		;GET 3RD ARG VAL CELL
	STE 1,ATOM		;ATOM?
	ERROR1 33,R		;NO, ILLEGAL ARG.
	GTVALC 2,1
	SKIPN 2
	CALL NEWVC1		;NO CELL - CREATE ONE
	MOVEI 3,(2)

;SETSTKARG(N POS VAL) - N NUM IS ARG#, N ATOME IS ARG NAME

SSTKAR:	CALL STKAR1
	CALL FNDBND		;POINT TO VALUE
	HRRZ 1,3(VP)
	HRRM 1,0(3)

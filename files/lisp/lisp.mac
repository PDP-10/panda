IFE TEN50,<	MOVE 2,0(1)
	MOVE 1,IOFNMP
	MOVE 3,FLTFMC
	TLNN F,PRXFLG		;USE STANDARD FORMAT
	HRLZI 3,4000		; WHEN FOR IPRE

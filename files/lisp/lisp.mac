	CALL GETMTH	;GET THE FN TO USE
	JRST APPBAD
	POP PP,2	;GET BACK THE OBJECT
	HRRZM 2,2(VP)	;AND PUT IT IN PLACE OF OPERATOR
	HRRZM 1,1(VP)	;NOW REDO WITH THE FN
	POP CP,1
	AOJA 1,APPLY.
>


;APPLY* FROM COMPILED CODE - STACK HAS FN AND ARGS - 1 HAS # ARGS

EVCC:	MOVNI 6,0(1)
	HRLI 6,PP
	HRRZ 2,@6		;GET FN
	PUSH PP,2		;AND SAVE IT
	PUSH CP,1		;SAVE # ARGS
	LDT 4,2
	CAIN 4,LISTT
	JRST EVCLS
IFDEF OOP,<
	SKIPE OBJTNS(4)
	JRST EVCOS
>
	CAIE 4,ATOMT
	JRST EVCBAD
	MOVEI 1,0(2)
	CALL ARGTYP
	JRST EVCBAD
	CAIE 1,3
	JRST EVCC1
	POP CP,7
	CALL EVCLA
	SKIPA 1,[1]
EVCC1:	POP CP,1
	POP PP,2		;FN
	JRST EVAF7		;CALL FN - AND RET TO POP OFF FN

EVCLS:	MOVEI 3,0(2)
	CARA 2,2
	CAMN 2,KFNARG
	JRST EVCFNA
	CALL ARGT2
	 JRST EVCBAD		;BAD EXPR
	CAIE 1,3
	JRST EVCLM1
	POP CP,7		;NO-SPREAD
	CALL EVCLA
	SKIPA 1,[1]
EVCLM1:	POP CP,1
	MOVEI 2,CLAM-1		;HOKEY FN NAME
	AOJA 1,EVAF7		;GO CALL W/ 1 MORE ARG

EVCFNA:	POP CP,7		;FUNARG
	CALL EVCLA		;PUNT - LIST ARGS
	POP PP,1
	POP PP,2
	SUB PP,BHC+1
	JRST APPLY


;LIST ARGS AND FLUSH FROM STACK

EVCLA:	HRRZ 1,KNIL
	JUMPE 7,EVCLAX		;NO ARGS
EVCLA1:	MOVEI 2,0(1)
	POP PP,1
	EXCH 1,0(PP)		;GET ARG AND SAVE FN
	CALL CONS
	SOJG 7,EVCLA1
EVCLAX:	EXCH 1,0(PP)		;SAVE LIST AND GET FN
	PUSH PP,1		;SAVE FN AGIN
	RET

EVCBAD:	POP CP,7
	CALL EVCLA
	SUB PP,BHC+1
	JRST APPB1

; APPLY* OF INSTANCES FROM COMPILED CODE

IFDEF OOP,<
EVCOS:	MOVE 4,0(CP)
	CAIGE 4,1
	JRST EVCBAD
	CDRA 2,2
	MOVNI 4,(4)
	HRLI 4,PP
	HRRZ 1,@4
	CALL GETMTH
	JRST EVCBAD
	MOVE 2,0(PP)
	POP CP,3
	MOVNI 6,(3)
	HRLI 6,PP
	HRRZM 2,@6
	SUB PP,BHC+1
	HRRZM 1,@6
	HRRZI 1,0(3)
	JRST EVCC

GETMTH:	PUSH PP,1		;SAVE TOKEN
	CALL GETHSH		;TRY GETHASH FIRST
	CAMN 1,KNIL
	JRST GTMTH1
	SUB PP,BHC+1		;SUCCESS! POP TOKEN, DONT NEED IT
	AOS 0(CP)		;SKIP RETURN
	RET
GTMTH1:	LDT 3,-1(PP)
	HRRZ 2,OBJTNS(3)	;GET THE FN
	JUMPE 2,GTMTH2		;NO FN - FAIL
	MOVEI 1,2		;WILL CALL IT WITH 2 ARGS
	HRRZ 3,-1(PP)		;WANT OBJECT AS FIRST ARG
	EXCH 3,0(PP)		;SO EXCHANGE WITH TOKEN
	PUSH PP,3
	CALL EFNCAL		;CALL THE USER'S METHOD FINDER
	CAME 1,KNIL		;GET A METHOD BACK?
	AOS 0(CP)		; YES - SKIP
	RET
GTMTH2:	SUB PP,BHC+1		;FLUSH THE UNNEEDED TOKEN
	RET			;AND DO A FAIL RETURN

#!/bin/sh
set -f
if test $# -lt 1; then
    echo "usage: $0 [-t] archive ..." 1>&2
    exit 1
  fi
OPTIND=1
ops=0
case $1 in
    -t)	echo "$0: operation not implemented" 1>&2
	exit 1;;
    -*)	echo "usage: $0 [-t] archive ..." 1>&2
	exit 1;;
  esac
files="`path20 -1 $*`"
for file in $files ; do
    echo "$file=$file/INDEX/EXIT"|maklib
  done

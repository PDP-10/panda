#!/bin/sh
set -f
if test $# -lt 1; then
    echo "usage: $0 [-agnopruw] files ..." 1>&2
    exit 1
  fi
args=''
case $1 in
    -*)	args=$1; shift;;
  esac
OPTIND=1
ops=0
while test -n "$args"; do
    case $args in
	a*|g*|n*|o*|p*|r*|u*|w*)
	    echo "$0: operation not implemented" 1>&2
	    exit 1;;
	?*)	echo "usage: $0 [-agnopruw] files ..." 1>&2
	    exit 1;;
      esac
    key=${key#?}
  done
files="`path20 -1 $*`"
for file in $files ; do
    echo "TTY:=$file/POINTS/EXIT" \
	| maklib | sed -e 's/[^ -}]$//' \
	| tr A-Z a-z \
	|sed -e '1,7d;s/^\([^	][^	]*\)/\
\1.rel:/;s/^	/00000000 T _/;s/	/\
00000000 T _/g'
  done
